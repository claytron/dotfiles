# Put the warning in a window back to normal (based on the tmuxline that was output)
setw window-status-format "#[fg=colour0,bg=colour0,nobold,nounderscore,noitalics]#[default] #I#F  #W #[fg=colour0,bg=colour0,nobold,nounderscore,noitalics]"
setw window-status-current-format "#[fg=colour0,bg=colour8,nobold,nounderscore,noitalics]#[fg=colour10,bg=colour8] #I#F  #W #[fg=colour8,bg=colour0,nobold,nounderscore,noitalics]"

# Put the warning in a window back to normal
setw window-status-current-format "#[fg=colour234,bg=colour33,nobold]⮀#[fg=colour254,bg=colour33,bold] #W ⮁ #I#F #[fg=colour33,bg=colour234,nobold]⮀"
setw window-status-format "#[fg=colour234,bg=colour240,nobold]⮀#[fg=colour244,bg=colour240,bold] #W ⮁ #I#F #[fg=colour240,bg=colour234,nobold]⮀"

--langdef=zcml
--langmap=zcml:.zcml
--regex-zcml=/.*name="(.*)"/\1/n,name/
--regex-zcml=/.*for="(.*)"?/\1/f,for/
--regex-zcml=/.*profile="(.*)"/\1/g,profile/
--regex-zcml=/.*permission="(.*)"/\1/p,permission/
--regex-zcml=/.*provides=".*\.(.[^"]*)"/\1/p,provides/
--regex-zcml=/.*id="(.*)"/\1/i,id/
--regex-zcml=/.*schema=".*\.(.[^"]*)"/\1/s,schema/
--regex-zcml=/.*handler=".*\.(.[^"]*)"/\1/h,handler/
--regex-zcml=/.*component=".*\.(.[^"]*)"/\1/m,component/
--regex-zcml=/.*factory=".*\.(.[^"]*)"/\1/f,factory/
--regex-zcml=/.*view=".*\.(.[^"]*)"/\1/v,view/
--regex-zcml=/.*class=".*\.(.[^"]*)"/\1/c,class/
--regex-zcml=/.*controller=".*\.(.[^"]*)"/\1/c,controller/
--regex-zcml=/.*type=".*\.(.[^"]*)"/\1/t,type/
--regex-zcml=/.*elector=".*\.(.[^"]*)"/\1/w,workflow/
--regex-zcml=/.*content_types=".*\.(.[^"]*)"/\1/w,workflow/
--regex-zcml=/.*initial_state=".*\.(.[^"]*)"/\1/w,workflow/
--regex-zcml=/.*state_attr=".*\.(.[^"]*)"/\1/w,workflow/
--regex-zcml=/.*permission_checker=".*\.(.[^"]*)"/\1/w,workflow/
--regex-zcml=/.*callback=".*\.(.[^"]*)"/\1/w,workflow/
--regex-zcml=/.*from_state=".*\.(.[^"]*)"/\1/w,workflow/
--regex-zcml=/.*to_state=".*\.(.[^"]*)"/\1/w,workflow/
--regex-zcml=/.*value=".*\.(.[^"]*)"/\1/w,workflow/
--regex-zcml=/.*title=".*\.(.[^"]*)"/\1/w,workflow/

--regex-python=/.*grok.name\('(.[^']*)'.*/\1/v,variables/
--regex-python=/.*grok.require\('(.[^']*)'.*/\1/v,variables/
--regex-python=/.*grok.subscribe\((.[^,]*),.*/\1/c,classes/

--langdef=ini
--langmap=ini:.cfg.ini
--regex-ini=/^\[(.*)\]/\1/s,section/

--langdef=markdown
--langmap=markdown:.md.mdown.mkdown.markdown
--regex-markdown=/^# (.*)(#)?/\1/1,header1/
--regex-markdown=/^#{2} (.*)(#{2})?/\1/2,header2/
--regex-markdown=/^#{3} (.*)(#{3})?/\1/3,header3/
--regex-markdown=/^#{4} (.*)(#{4})?/\1/4,header4/
--regex-markdown=/^#{5} (.*)(#{5})?/\1/5,header5/
--regex-markdown=/^#{6} (.*)(#{6})?/\1/6,header6/
--regex-markdown=/^#{7} (.*)(#{7})?/\1/7,header7/

--langdef=yaml
--langmap=yaml:.yml.sls
--regex-yaml=/^([a-zA-Z0-9_\/-]+):/\1/d,definition/
--regex-yaml=/.*- name: (.*)/\1/n,name/
